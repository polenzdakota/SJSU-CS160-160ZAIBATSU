<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>CardSearch</servlet-name>
        <servlet-class>Controllers.CardSearch</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserVerify</servlet-name>
        <servlet-class>Controllers.UserVerify</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getCardAttributes</servlet-name>
        <servlet-class>Controllers.getCardAttributes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FindCard</servlet-name>
        <servlet-class>Controllers.FindCard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchCard</servlet-name>
        <servlet-class>Models.SearchCard</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Card</servlet-name>
        <servlet-class>Models.Card</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CardSearch</servlet-name>
        <url-pattern>/CardSearch</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserVerify</servlet-name>
        <url-pattern>/UserVerify</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getCardAttributes</servlet-name>
        <url-pattern>/getCardAttributes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FindCard</servlet-name>
        <url-pattern>/FindCard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchCard</servlet-name>
        <url-pattern>/SearchCard</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Card</servlet-name>
        <url-pattern>/Card</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
